<div class="create-view" layout="column">
  <md-toolbar>
    <div class="md-toolbar-tools">
      <md-button ng-click="ctrl.onBackClick()">
        back
      </md-button>
      <h2>{{ctrl.getAssetName()}}</h2>
    </div>
  </md-toolbar>
  <md-tabs md-border-bottom flex md-selected="ctrl.getSelectedTab()">
    <md-tab class="md-display-2" label="1. load">
      <pc-load-tab asset="ctrl.getAsset()"></pc-load-tab>
    </md-tab>
  </md-tabs>
</div>
