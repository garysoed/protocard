<md-content class="pc-partial pc-fill-parent" layout="column" layout-align="start end">
  <md-list>
    <md-list-item ng-repeat="(name, partials) in ctrl.partials">
      <pc-partial-item asset="ctrl.asset" name="name"></pc-partial-item>
    </md-list-item>
  </md-list>
</md-content>
<md-button ng-click="ctrl.onAddClick()" class="pc-context-button md-fab">
  <md-icon>add</md-icon>
</md-button>
