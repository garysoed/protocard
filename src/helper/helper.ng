<md-content class="pc-helper pc-fill-parent" layout="column" layout-align="start end">
  <md-list>
    <md-list-item ng-repeat="(name, helper) in ctrl.helpers">
      <pc-helper-item helper="helper" name="name"></pc-helper-item>
    </md-list-item>
  </md-list>
</md-content>
<md-button ng-click="ctrl.onAddClick()" class="pc-context-button md-fab">
  <md-icon>add</md-icon>
</md-button>
