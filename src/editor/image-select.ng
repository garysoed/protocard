<md-grid-list
    class="image-select"
    md-cols="5"
    md-row-height="2:2"
    md-gutter="6px">
  <md-grid-tile ng-repeat="image in ctrl.images" ng-click="ctrl.select(image)">
    <img src="{{image.previewUrl}}">
    <md-icon ng-if="ctrl.isSelected(image)">check_circle</md-icon>
    <md-grid-tile-footer>
      {{image.alias}}
    </md-grid-tile-footer>
  </md-grid-tile>
</md-grid-list>
